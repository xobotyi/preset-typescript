{
  "compilerOptions": {
    "declaration": false,
    "sourceMap": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,

    "removeComments": false,

    "noImplicitAny": false,

    "strict": true,
    "alwaysStrict": true,

    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "lib": [
      "esnext",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
