{
  "compilerOptions": {
    "sourceMap": true,
    "removeComments": false,
    "alwaysStrict": true,
    "declaration": false,
    "noImplicitAny": false,
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "resolveJsonModule": true,
    "lib": [
      "esnext",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
